{
  "expo": {
    "name": "zetheq",
    "slug": "zetheq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.gif",
      "resizeMode": "contain",
      "backgroundColor": "#010e19"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#010e19"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#010e19"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/RobotoBlack.ttf",
            "./assets/fonts/RobotoBold.ttf",
            "./assets/fonts/RobotoLight.ttf",
            "./assets/fonts/RobotoMedium.ttf",
            "./assets/fonts/RobotoRegular.ttf",
            "./assets/fonts/RobotoThin.ttf"
          ]
        }
      ]
    ]
  }
}
